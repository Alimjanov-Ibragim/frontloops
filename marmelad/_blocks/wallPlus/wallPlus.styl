.wallPlus
    content ''
    position absolute
    top 34px
    right 16px
    margin 0
    padding 0
    width 40px
    height 40px
    border none
    outline none
    border 1px solid transparent
    border-radius 50%
    background none
    cursor pointer
    
    &:hover,
    &:active
        border 1px solid #e6e4e4
        background #fff
    &:focus
        border 1px solid #949393
        background #fff